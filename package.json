{
  "name": "kitkat-bot-core",
  "version": "1.0.1",
  "description": "The bread and butter of kitkat-bot!",
  "main": "src/index.js",
  "homepage": "https://github.com/butter-cookie-kitkat/kitkat-bot-core",
  "bugs": "https://github.com/butter-cookie-kitkat/kitkat-bot-core/issues",
  "contributors": [
    "Nick Woodward <nick@salte.io>"
  ],
  "scripts": {
    "test": "mocha",
    "test:coverage": "nyc mocha",
    "test:tdd": "mocha --watch",
    "lint": "eslint --ignore-path .gitignore bot/**/*.js",
    "lint:fix": "eslint --ignore-path .gitignore --fix"
  },
  "license": "MIT",
  "dependencies": {
    "@discordjs/opus": "^0.3.2",
    "debug": "^4.1.1",
    "discord.js": "^12.2.0",
    "ffmpeg-static": "^4.2.2",
    "outdent": "^0.7.1",
    "pad-left": "^2.1.0",
    "yargs-parser": "^18.1.3",
    "ytdl-core": "^3.1.1",
    "ytdl-core-discord": "^1.2.0"
  },
  "devDependencies": {
    "chance": "^1.1.6",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.2.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-jsdoc": "^27.0.4",
    "esm": "^3.2.25",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "mocha": "^7.2.0",
    "nyc": "^15.1.0",
    "sinon": "^9.0.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "npm run lint:fix",
      "git add --force"
    ],
    "*.json": [
      "git add --force"
    ]
  }
}
